package lthn19;
import java.util.Arrays;

public class lthn19 {
    public static void main(String[] args) {
        int[] angka = {5, 2, 8, 1, 7};
        Arrays.sort(angka);
        System.out.print("Angka terurut: ");
        for (int num : angka) {
            System.out.print(num + " ");
        }
    }
}
