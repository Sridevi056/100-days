package ltihan10;
public class ltihan10 {
    public static void main(String[] args) {
        int[] arr = {17, 12, 25, 31, 8};
        int max = arr[0];
        for (int i = 1; i < arr.length; i++) {
            if (arr[i] > max) {
                max = arr[i];
            }
        }
        System.out.println("Nilai terbesar dalam array: " + max);
    }
}
