package tgs1;
public class tgs1 {
    public static void main(String[] args) {
        int choice = 2;

        switch (choice) {
            case 1:
                System.out.println("Anda memilih pilihan 1");
                break;
            case 2:
                System.out.println("Anda memilih pilihan 2");
                break;
            case 3:
                System.out.println("Anda memilih pilihan 3");
                break;
            default:
                System.out.println("Pilihan tidak valid");
        }
    }
}
