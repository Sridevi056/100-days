public class day95 {
    public static void main(String[] args) {
        int[] angka1 = {1, 2, 3, 4, 5};
        int[] angka2 = {4, 5, 6, 7, 8};
        int n1 = angka1.length;
        int n2 = angka2.length;
        int count = 0;
        for (int i = 0; i < n1; i++) {
            for (int j = 0; j < n2; j++) {
                if (angka1[i] == angka2[j]) {
                    count++;
                    break;
                }
            }
        }
        System.out.println("Jumlah elemen yang sama dalam dua array: " + count);
    }
}
