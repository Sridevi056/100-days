package lthn22;
public class lthn22 {
	public static void main(String[] args) {
		
        int[][] matriks = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}};
        int baris = matriks.length;
        int kolom = matriks[0].length;

        for (int i = 0; i < kolom; i++) {
            for (int j = 0; j < baris; j++) {
                System.out.print(matriks[j][i] + " ");
            }
            System.out.println();
        }
    }
}
