import java.util.LinkedList;
import java.util.Queue;

public class Main {
    public static void main(String[] args) {
        Queue<Integer> queue = new LinkedList<>();

        // Menambahkan elemen ke dalam antrian
        queue.add(1);
        queue.add(2);
        queue.add(3);

        System.out.println("Antrian saat ini: " + queue);

        // Menghapus elemen pertama dari antrian
        int removedItem = queue.poll();
        System.out.println("Elemen yang dihapus: " + removedItem);
        System.out.println("Antrian setelah penghapusan: " + queue);

        // Menambahkan elemen baru ke dalam antrian
        queue.add(4);
        System.out.println("Antrian setelah penambahan: " + queue);
    }
}
